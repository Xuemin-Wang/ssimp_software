"$SSIMP"                                                                            \
    --ref "$REF"/sub1KG-tiny/chr{CHRM}.vcf.gz                                          \
    --wind 300000                                                                   \
    --gwas "$GWAS"/GIANT_HEIGHT_Wood_et_al_2014_publicrelease_HapMapCeuFreq.txt.gz  \
    --out "${OUT_IMPUTATIONS}"                                                      \
    --lambda 0.01                                                                   \
    --tags.snps   "${COMMANDDIR}"/a.list.of.snps.txt                                 \
    --impute.range chr1-chr3
    > "$OUTPUT"
