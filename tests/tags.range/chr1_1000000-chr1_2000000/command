"$SSIMP"                                                                            \
    --ref "$REF"/sub1KG-tiny/chr{CHRM}.vcf.gz                                          \
    --wind 300000                                                                   \
    --gwas "$GWAS"/GIANT_HEIGHT_Wood_et_al_2014_publicrelease_HapMapCeuFreq.txt.gz  \
    --out "${OUT_IMPUTATIONS}"                                                      \
    --lambda 0.01                                                                   \
    --tags.used.output "${COMMANDDIR}"/tag.data.used.here.tsv                       \
    --tag.range  chr1:18016491-chr1:18490574   \
    --_debug_build_chr 1                                                            \
    > "$OUTPUT"
