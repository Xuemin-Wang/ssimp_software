"$SSIMP"                                                                                \
    --ref "$REF"/tbi/small.vcf.sample.vcf.gz                                            \
    --wind 300000                                                                       \
    --gwas "$GWAS"/small.UKBB.both.csv                                                  \
    --lambda 1.0                                                                        \
    --impute.range chr22:16287000-chr22:16287600                                        \
    > "$OUTPUT"
